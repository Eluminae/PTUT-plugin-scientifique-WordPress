    </custom-fields>
    <custom-types>
        <custom-type translate="0">clients</custom-type>
        <custom-type translate="1">price</custom-type>
        <custom-type translate="1">services</custom-type>
        <custom-type translate="1">stats</custom-type>
        <custom-type translate="1">team</custom-type>
        <custom-type translate="1">testimonials</custom-type>
        <custom-type translate="1">toggle</custom-type>
